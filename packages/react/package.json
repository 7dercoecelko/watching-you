{
  "name": "@gazer/react",
  "private": true,
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "main": "./dist/react-gazer.umd.cjs",
  "module": "./dist/react-gazer.js",
  "types": "./index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/react-gazer.js",
      "require": "./dist/react-gazer.umd.cjs"
    },
    "./umd": "./dist/react-gazer.umd.cjs"
  },
  "keywords": [
    "react",
    "animation",
    "react-component"
  ],
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "format": "prettier --write 'src/**/**.{ts,tsx,js,cjs,mjs}'",
    "tsc": "tsc --noEmit",
    "lint": "eslint 'src/**/*.{ts,tsx,js,cjs,mjs}' --max-warnings=0"
  },
  "devDependencies": {
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.6.4",
    "vite": "^3.1.0",
    "vite-tsconfig-paths": "^3.5.0"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17 || ^18",
    "react-dom": "^16.8.0 || ^17 || ^18"
  }
}
